<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pms.dao.WorkPageDao">
	<select id="getWorkPageList" resultType="workpage" parameterType="int">
		select * 
		from jobplan
		WHERE pid = #{pid}
	</select>

	<insert id="insertFile" parameterType="workpagefile">
		insert into jobfile values(jobfile_seq.currval,
		#{jmid},sysdate,#{PATH},#{fname});
	</insert>
	
	<select id="getWokrPageDetail" resultType="workpage" parameterType="int" >
		select * 
		from jobplan
		WHERE pid = #{pid}
	</select>
</mapper>
        
        